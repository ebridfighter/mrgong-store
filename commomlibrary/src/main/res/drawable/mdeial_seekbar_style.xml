<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:angle="270"
                android:centerColor="#555555"
                android:centerY="0.45"
                android:endColor="#555555"
                android:startColor="#555555"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <bitmap android:src="@drawable/medial_progressbar"/>
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="#FF75BC32"
                    android:centerY="0.45"
                    android:endColor="#FF75BC32"
                    android:startColor="#FF75BC32" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
          <shape>
           <solid android:color="#0000"/>
        </shape>
    </item>

</layer-list>